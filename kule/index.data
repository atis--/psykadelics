local function PadZeros(number, amount)
	local padded = ""
	amount = amount - 1
	local base = math.pow(10, amount)
	for i = 1, amount, 1 do
		if number / base >= 1.0 then break end
		padded = padded .. "0"
		base = base / 10
	end
	return padded .. number
end

