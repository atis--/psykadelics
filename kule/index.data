local function Repeater(FN, times, interval, body, delay)
	body = body or staticBody
	local function Repeat()
		FN()
		times = times - 1
		if times > 0 then
			eapi.AddTimer(body, interval, Repeat)
		end
	end
	eapi.AddTimer(body, delay or 0, Repeat)
end

